{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "seed:variants": "ts-node -r tsconfig-paths/register src/cli/seed-variants.ts",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "prisma:generate": "prisma generate --schema ../../packages/db/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ../../packages/db/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ../../packages/db/prisma/schema.prisma",
    "worker:processing": "ts-node -r tsconfig-paths/register src/workers/processing.worker.ts"
  },
  "dependencies": {
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.22.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.11.0",
    "execa": "^9.6.0",
    "form-data": "^4.0.4",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.9.0",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
